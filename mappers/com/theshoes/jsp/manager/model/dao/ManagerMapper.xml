<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.theshoes.jsp.manager.model.dao.ManagerMapper">
	
	<resultMap type="com.theshoes.jsp.board.model.dto.BoardDTO" id="boardResultMap">
		<id property="boardNo" column="BOARD_NO"/>
		<result property="boardId" column="BOARD_ID"/>
		<result property="boardCategoryNo" column="BOARD_CATEGORY_NO"/>
		<result property="boardTitle" column="BOARD_TITLE"/>
		<result property="boardContent" column="BOARD_CONTENT"/>
		<result property="boardRegDate" column="BOARD_REGDATE"/>
		<result property="boardHit" column="BOARD_HIT"/>
		<result property="categoryOrder" column="CATEGORY_ORDER"/>
	</resultMap>
	
	<insert id="insertModel" parameterMap="com.theshoes.jsp.board.model.dto.BoardDTO">
		INSERT
			  INTO BOARD A
			(
			  A.BOARD_NO
			, A.BOARD_ID
			, A.BOARD_CATEGORY_NO
			, A.BOARD_TITLE
			, A.BOARD_CONTENT
			, A.BOARD_REGDATE
			, A.BOARD_HIT
			, A.CATEGORY_ORDER
			)
			VALUES
			(
			  SEQ_BOARD_CODE.NEXTVAL
			, 'USER03'
			, 1
			, '12번 공지사항 제목'
			, '12번 공지사항 내용'
			, SYSDATE
			, 0
			, 12
			)
	</insert>
	

</mapper>
